{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": ["off", {
      "endOfLine": "off"
    }],
    "react/prop-types": "off",
    "no-var": "off",
    "no-dupe-keys": "off",
    "react/react-in-jsx-scope":"off",
    "react/no-unknown-property" : "off",
    "react/jsx-key":"off"
  }
}
